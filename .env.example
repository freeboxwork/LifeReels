REMOTION_AWS_SECRET_ACCESS_KEY=YOUR_AWS_SECRET_ACCESS_KEY
REMOTION_AWS_SESSION_TOKEN=
REMOTION_FUNCTION_NAME=remotion-render-xxxxxxx
REMOTION_SERVE_URL=https://remotionlambda-xxxxxxx.s3.us-east-1.amazonaws.com/sites/xxxxxxx/index.html
